<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#f6f9fc"
    tools:context="com.example.dell.designapp.multiplexer.example.dell.designapp.fragment.AccountFragment"
    android:id="@+id/relativelayout"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:height="47dp"
                android:background="@color/colorPrimaryDark" />

            <at.markushi.ui.CircleButton
                android:layout_height="21dp"
                android:layout_width="23dp"
                android:id="@+id/menu"
                android:layout_marginTop="5dp"
                app:cb_color="@color/colorPrimaryDark"
                android:src="@drawable/ic_menu_black_24dp"
                android:layout_marginLeft="20dp"
                />

            <TextView
                android:id="@+id/accounts"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="20dp"
                android:text="Accounts"
                android:layout_marginLeft="10dp"
                android:textColor="#ffffff"
                android:gravity="center"
                />

            <at.markushi.ui.CircleButton
                android:layout_height="21dp"
                android:layout_width="23dp"
                android:layout_marginTop="5dp"
                android:id="@+id/plus"
                app:cb_color="@color/colorPrimaryDark"
                android:layout_toRightOf="@id/menu"
                android:src="@drawable/ic_add_black_24dp"
                android:layout_marginLeft="296dp"
                />

            <TextView
                android:id="@+id/availability"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:layout_marginTop="205dp"
                android:text="Availability"
                android:layout_below="@id/accounts"
                android:gravity="center"
                android:textColor="#4f6c8d"
                />

            <TextView
                android:id="@+id/dollar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="50dp"
                android:text="2,950"
                android:layout_below="@id/availability"
                android:gravity="center"
                android:layout_marginBottom="25dp"
                android:textColor="#4f6c8d"
                />

            <TextView
                android:id="@+id/dollarsign"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:layout_marginTop="16dp"
                android:text="$"
                android:layout_below="@id/availability"
                android:gravity="left"
                android:layout_marginLeft="107dp"
                android:layout_marginBottom="15dp"
                android:textColor="#a7b5c6"
                />

            <TextView
                android:id="@+id/cents"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:layout_marginTop="16dp"
                android:text=".00"
                android:layout_below="@id/availability"
                android:gravity="end"
                android:layout_marginRight="95dp"
                android:layout_marginBottom="15dp"
                android:textColor="#a7b5c6"
                />

            <TextView
                android:id="@+id/white"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:height="335dp"
                android:layout_below="@id/dollar"
                android:background="#ffffff" />

            <TextView
                android:id="@+id/applestore"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:layout_marginTop="15dp"
                android:text="Apple Store"
                android:layout_below="@id/dollar"
                android:layout_marginLeft="10dp"
                android:textColor="#4f6c8d"
                />

            <TextView
                android:id="@+id/dateone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:layout_marginTop="10dp"
                android:text="03/08/2016"
                android:layout_below="@id/applestore"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="15dp"
                android:textColor="#a3adb4"
                />

            <TextView
                android:id="@+id/billone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="30dp"
                android:layout_marginTop="16dp"
                android:text="- 299"
                android:layout_below="@id/dollar"
                android:gravity="end"
                android:layout_marginRight="40dp"
                android:layout_marginBottom="15dp"
                android:textColor="#f96b6b"
                />

            <TextView
                android:id="@+id/billonesmall"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:layout_marginTop="22dp"
                android:text=".95"
                android:layout_below="@id/dollar"
                android:gravity="end"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="15dp"
                android:textColor="#f96b6b"
                />
            <View
                android:layout_width="match_parent"
                android:id="@+id/listone"
                android:layout_height="1dp"
                android:padding="5dp"
                android:layout_below="@id/dateone"
                android:background="#bec9d1" />

            <TextView
                android:id="@+id/ducatitrumpnewyork"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:layout_marginTop="15dp"
                android:text="Ducati Trump New York"
                android:layout_below="@id/listone"
                android:layout_marginLeft="10dp"
                android:textColor="#4f6c8d"
                />

            <TextView
                android:id="@+id/datetwo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:layout_marginTop="10dp"
                android:text="02/16/2016"
                android:layout_below="@id/ducatitrumpnewyork"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="15dp"
                android:textColor="#a3adb4"
                />

            <TextView
                android:id="@+id/billtwo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="30dp"
                android:layout_marginTop="29dp"
                android:text="- 1,182"
                android:layout_below="@id/billone"
                android:gravity="end"
                android:layout_marginRight="40dp"
                android:layout_marginBottom="15dp"
                android:textColor="#f96b6b"
                />

            <TextView
                android:id="@+id/billtwosmall"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:layout_marginTop="21dp"
                android:text=".00"
                android:layout_below="@id/listone"
                android:gravity="end"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="15dp"
                android:textColor="#f96b6b"
                />
            <View
                android:layout_width="match_parent"
                android:id="@+id/listtwo"
                android:layout_height="1dp"
                android:padding="5dp"
                android:layout_below="@id/datetwo"
                android:background="#bec9d1" />

            <TextView
                android:id="@+id/yebocreative"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:layout_marginTop="15dp"
                android:text="Yebo Creative"
                android:layout_below="@id/listtwo"
                android:layout_marginLeft="10dp"
                android:textColor="#4f6c8d"
                />

            <TextView
                android:id="@+id/datethree"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:layout_marginTop="10dp"
                android:text="02/06/2016"
                android:layout_below="@id/yebocreative"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="15dp"
                android:textColor="#a3adb4"
                />

            <TextView
                android:id="@+id/billthree"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="30dp"
                android:layout_marginTop="29dp"
                android:text="- 99"
                android:layout_below="@id/billtwo"
                android:gravity="end"
                android:layout_marginRight="40dp"
                android:layout_marginBottom="15dp"
                android:textColor="#f96b6b"
                />

            <TextView
                android:id="@+id/billthreesmall"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:layout_marginTop="22dp"
                android:text=".99"
                android:layout_below="@id/listtwo"
                android:gravity="end"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="15dp"
                android:textColor="#f96b6b"
                />
            <View
                android:layout_width="match_parent"
                android:id="@+id/listthree"
                android:layout_height="1dp"
                android:padding="5dp"
                android:layout_below="@id/datethree"
                android:background="#bec9d1" />

            <TextView
                android:id="@+id/marvelprototyping"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:layout_marginTop="15dp"
                android:text="Marvel Prototyping"
                android:layout_below="@id/listthree"
                android:layout_marginLeft="10dp"
                android:textColor="#4f6c8d"
                />

            <TextView
                android:id="@+id/datefour"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="16dp"
                android:layout_marginTop="10dp"
                android:text="02/06/2016"
                android:layout_below="@id/marvelprototyping"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="15dp"
                android:textColor="#a3adb4"
                />

            <TextView
                android:id="@+id/billfour"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="30dp"
                android:layout_marginTop="29dp"
                android:text="- 42"
                android:layout_below="@id/billthree"
                android:gravity="end"
                android:layout_marginRight="40dp"
                android:layout_marginBottom="15dp"
                android:textColor="#f96b6b"
                />

            <TextView
                android:id="@+id/billfoursmall"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textSize="14dp"
                android:layout_marginTop="22dp"
                android:text=".00"
                android:layout_below="@id/listthree"
                android:gravity="end"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="15dp"
                android:textColor="#f96b6b"
                />

        </RelativeLayout>
    </ScrollView>

</RelativeLayout>